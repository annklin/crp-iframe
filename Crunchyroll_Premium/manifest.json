{
  "name": "Crunchyroll iFrame Player",
  "version": "1.2.2",
  "description": "Permite ver todos os v√≠deos do crunchyroll gratuitamente.",
  "author": "itallolegal",
  "icons": {
    "48": "icon.png"
  },
  "action": {
    "default_title": "Crunchyroll iFrame Player",
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "web_accessible_resources": [{
    "resources": ["blockevidon.js"],
    "matches": ["<all_urls>"]
  }],
  "host_permissions": [
    "https://pl.crunchyroll.com/*",
    "https://www.crunchyroll.com/*",
    "https://*.mateus7g.github.io/*",
    "https://*.v.vrv.co/*",
    "http://localhost/*"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "crpcors",
        "enabled": true,
        "path": "cors.json"
      }
    ]
  },
  "content_scripts": [
    {
      "matches": [ "https://*.crunchyroll.com/*" ],
      "css": [ "content.css" ],
      "js": [ "content.js" ],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 3
}